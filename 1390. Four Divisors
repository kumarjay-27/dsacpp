class Solution {
public:
    int sumFourDivisors(vector<int>& nums) {
        int total=0;
        for(int x:nums){
            int n=0;
            int sum=0; 
            for (int i = 1; i * i <= x; i++) {
                if (x % i == 0) {
                    int d1 = i;
                    int d2 = x / i;

                    sum += d1;
                    n++;

                    if (d1 != d2) {
                        sum += d2;
                        n++;
                    }
                }
            }
            if(n==4) total=total +sum;
        }
        return total;

    }
};
