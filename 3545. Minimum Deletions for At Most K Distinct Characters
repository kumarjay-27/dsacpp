class Solution {
public:
    int minDeletion(string s, int k) {
       unordered_map<char,int> m;
       for(char x:s){
        m[x]++;
       }
       if(m.size()<=k) return 0;
       vector<int> counts;
        for (auto it : m) {
            counts.push_back(it.second); 
        }
        sort(counts.begin(),counts.end());
        int res=0;
        int extra=m.size()-k;
        for(int i=0;i<extra;i++){
            res= res+counts[i];
        }
    return res;
    }
};
