class Solution {
public:
    bool kLengthApart(vector<int>& nums, int k) {
        bool first = true;
        int cnt=0;
        for(int i=0;i<nums.size();i++){
            if(nums[i]==1){
                if(!first && cnt<k) return false;
                first= false;
                cnt=0;
            }
            else cnt++;
        }
        return true;
        
    }
};
