class Solution {
public:
    long long subArrayRanges(vector<int>& nums) {
        long long total=0;
        for(int i=0;i<nums.size()-1;i++)
        {
           int small=nums[i];
           int large=nums[i];
           for(int j=i+1;j<nums.size();j++){
            small=min(nums[j],small);
            large=max(nums[j],large);
            total=total+(large-small);
        }
        }
        return total;
        
    }
};
