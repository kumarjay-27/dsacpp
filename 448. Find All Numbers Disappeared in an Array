class Solution {
public:
    vector<int> findDisappearedNumbers(vector<int>& nums) {
        // find() use karenge exceed time so use binary_search()
        sort(nums.begin(),nums.end());
        int val=1;
        vector<int> result;
        while(val<=nums.size()){
            if(!binary_search(nums.begin(),nums.end(),val)) result.push_back(val);
            val++;
        }
        return result;
    }
};
