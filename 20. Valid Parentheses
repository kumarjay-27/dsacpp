class Solution {
public:
    bool isValid(string s) {
        stack<char>ch;
        bool answer=true;
        for(int i=0;i<s.length();i++){
            if(s[i]=='('||s[i]=='{'||s[i]=='[') ch.push(s[i]);
            else if (s[i] == ')' || s[i] == '}' || s[i] == ']') {

                if (ch.empty()) {         
                    answer = false;
                    break;
                }

                char top = ch.top();     
                ch.pop();                
                if ((s[i] == ')' && top != '(') ||
                    (s[i] == '}' && top != '{') ||
                    (s[i] == ']' && top != '[')) {
                    answer = false;
                    break;
                    }
            }

        }
        if(!ch.empty()) return false;
        return answer;
        
    }
};
