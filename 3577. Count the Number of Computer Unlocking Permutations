class Solution {
public:
    int countPermutations(vector<int>& complexity) {
        long long result=1;
        int M=1E9+7;
        for(int i=1;i<complexity.size();i++){
            if(complexity[i]<=complexity[0]) return 0;
            result= result*(i)%M;
        }
        return result;
        
    }
};
