class Solution {
public:
    int countNegatives(vector<vector<int>>& grid) {
        int ans=0;
        for(int i=0;i<grid.size();i++){
            for(int j=0;j<grid[i].size();j++){ // here i used grid[i].size which give me the number of element in the row
                if(grid[i][j]<0) ans++;
            }
        }
        return ans;
    }
};
