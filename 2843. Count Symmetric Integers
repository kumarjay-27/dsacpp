class Solution {
public:
    int countSymmetricIntegers(int low, int high) {
        int answer=0;
        for(int i=low ;i<=high;i++){
            string s=to_string(i);
            int n=s.length();
            if(n%2==0){
                int sum=0,sum1=0;
                 for(int j=0;j<n/2;j++)
                 {
                    sum= sum+s[j];
                 }
                 for(int k=(n/2);k<n;k++)
                 {
                    sum1= sum1+s[k];
                 }
                 if(sum==sum1) answer++;
            }
            else continue;


        }
        return answer;
    }
};
