class Solution {
public:
    int majorityElement(vector<int>& nums) {
        int ct=0;
        int el;
        for(int i=0;i<nums.size();i++)
        {
            if(ct==0){
                el=nums[i];
                ct=1;
            }
            else if(el==nums[i]){
                ct++;
            }
            else ct--;
        }
        int ctt=0;
        for(int i=0;i<nums.size();i++){
            if(el==nums[i]) ctt++;
        }
        if(ctt>(nums.size()/2)) return el;
        else return -1;
        
    }
};
