class Solution {
public:
    int countBinarySubstrings(string s) {
        int val=0;
        int curr=1;
        int prev=0;
        for (int i=1;i<s.length();i++){
            if(s[i]==s[i-1]){
                curr++;
            }
            else {
                val += min(curr,prev);
                prev=curr;
                curr=1;
            }
        }
        return val+min(curr,prev);
    }
};
